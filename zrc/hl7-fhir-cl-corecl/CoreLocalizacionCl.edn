{ns hl7-fhir-cl-corecl.CoreLocalizacionCl,
 import #{zen.fhir
          hl7-fhir-cl-corecl.value-set.VSCodigosProvinciasCL
          hl7-terminology-r4.value-set.v3-ServiceDeliveryLocationRoleType
          hl7-fhir-cl-corecl.value-set.VSCodigosRegionesCL
          hl7-fhir-r4-core.value-set.contact-point-use
          hl7-fhir-r4-core.value-set.contact-point-system
          hl7-fhir-cl-corecl.value-set.VSCodigosComunaCL
          hl7-fhir-r4-core.Location
          hl7-fhir-r4-core.value-set.location-status
          hl7-fhir-cl-corecl.value-set.CodPais},
 schema {:zen/tags #{zen.fhir/profile-schema zen/schema},
         :zen/desc "Este perfil determina la ubicación especifica para un evento, objeto o Infraestructura específica. Este perfil permite vinlcular la localización especifica con una organización o espacio físico específico",
         :zen.fhir/type "Location",
         :zen.fhir/profileUri "https://hl7chile.cl/fhir/ig/CoreCL/StructureDefinition/CoreLocalizacionCl",
         :zen.fhir/version "0.6.30-13",
         :confirms #{hl7-fhir-r4-core.Location/schema
                     zen.fhir/Resource},
         :type zen/map,
         :keys {:address {:type zen/map,
                          :keys {:use {:fhir/flags #{:MS}},
                                 :city {:fhir/flags #{:MS},
                                        :zen.fhir/value-set {:symbol hl7-fhir-cl-corecl.value-set.VSCodigosComunaCL/value-set,
                                                             :strength :required},
                                        :zen/desc "Campo para Comuna de residencia"},
                                 :state {:fhir/flags #{:MS},
                                         :zen.fhir/value-set {:symbol hl7-fhir-cl-corecl.value-set.VSCodigosRegionesCL/value-set,
