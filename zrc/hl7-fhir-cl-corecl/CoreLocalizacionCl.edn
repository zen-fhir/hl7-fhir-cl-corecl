                :availabilityExceptions {:fhir/flags #{:MS},
                                         :zen/desc "Descripción de Excepciones de ubicación habilitada."},
                :position {:type zen/map,
                           :keys {:longitude {:fhir/flags #{:MS},
                                              :zen/desc "Longitud valor decimal en WGS84 datum (World Geodetic System 1984)"},
                                  :latitude {:fhir/flags #{:MS},
                                             :zen/desc "Latitud valor decimal en formato WGS84 datum (World Geodetic System 1984)"}},
                           :fhir/flags #{:MS},
                           :zen/desc "Geoposisionamiento del objeto, infraestructura o evento en longitud y latitud"},
